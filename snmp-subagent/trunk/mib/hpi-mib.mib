hpiInventory	OBJECT IDENTIFIER ::= { hpiResources 8 }

saHpiInventoryCount OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The number of inventory resource data records present on this system."
	::= {hpiInventory 1}

saHpiInventoryTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SaHpiInventoryEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Table defining entity inventory resource data records."
	::= {hpiInventory 2}
	
saHpiInventoryEntry OBJECT-TYPE
	SYNTAX		SaHpiInventoryEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
	"These structures are used to read and write inventory data to entity inventory
	repositories within a resource.
		
	All inventory data contained in an inventory data repository (IDR)
	must be represented in the RDR repository"
	
	INDEX	{ 
		saHpiDomainId, 
		saHpiResourceId, 
		saHpiInstrumentId
	}
	::= {saHpiInventoryTable 1}

SaHpiInventoryEntry ::= SEQUENCE {
	saHpiIdrPersistent              TruthValue,
	saHpiInventoryOEM		Unsigned32,
	saHpiIdrUpdateCount		Unsigned32,
	saHpiIdrIsReadOnly		TruthValue,
	saHpiIdrNumAreas		Unsigned32,
	saHpiInventoryRDR		RowPointer
	}
	
saHpiIdrPersistent OBJECT-TYPE 
	SYNTAX		TruthValue
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	
	"True indicates that updates to IDR are automatically and immediately 
	persisted. False indicates that updates are notimmediately persisted; 
	but optionally may be persisted via saHpiParmControl function, as
        defined in implementation documentation."
    	REFERENCE
		""
	::= {saHpiInventoryEntry 1}

saHpiInventoryOEM OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
	"OEM value for the inventory record. This is manufacturere' 
	specific value."
	::= {saHpiInventoryEntry 2}

saHpiIdrUpdateCount OBJECT-TYPE
    SYNTAX	Unsigned32
    MAX-ACCESS	read-only
    STATUS	current
    DESCRIPTION
	"The count is incremented any time the IDR is changed."

    ::= {saHpiInventoryEntry 3}

saHpiIdrIsReadOnly OBJECT-TYPE
    SYNTAX	TruthValue
    MAX-ACCESS	read-only
    STATUS	current
    DESCRIPTION
	"Describes if the IDR is read-only. If such, all area
	headers and fields are flagged as well."
    REFERENCE
    	" "
    ::= {saHpiInventoryEntry 4}


saHpiIdrNumAreas OBJECT-TYPE
    SYNTAX 	Unsigned32
    MAX-ACCESS	read-only
    STATUS	current
    DESCRIPTION
	"Number of area containted in IDR"
    REFERENCE
    	" "
    ::= {saHpiInventoryEntry 5}


saHpiInventoryRDR OBJECT-TYPE
        SYNTAX          RowPointer
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Pointer to resource data record for this particulat inventory resource data record"
        ::= {saHpiInventoryEntry 6}
	
