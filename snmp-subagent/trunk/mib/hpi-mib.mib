saHpiInventoryCount OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The number of inventory resource data records present on this system."
	::= {hpiResources 8}

saHpiInventoryTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SaHpiInventoryEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"Table defining entity inventory resource data records."
	::= {hpiResources 9}
saHpiInventoryEntry OBJECT-TYPE
	SYNTAX		SaHpiInventoryEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"These structures are used to read and write inventory data to entity inventory
		repositories within a resource."
	INDEX	{ saHpiDomainID, saHpiResourceID, saHpiInventoryEirId, saHpiInventoryIndex }
	::= {saHpiInventoryTable 1}


SaHpiInventoryEntry ::= SEQUENCE {
	saHpiInventoryEirId		Unsigned32,
	saHpiInventoryIndex		Unsigned32,
	saHpiInventoryRecordType	INTEGER,
	saHpiInventoryValidity		INTEGER,
	saHpiInventoryAttributes	OCTET STRING,
	saHpiInventoryTextType		INTEGER,
	saHpiInventoryTextLanguage	INTEGER,
	saHpiInventoryManufacturer	OCTET STRING,
	saHpiInventoryProductName	OCTET STRING,
	saHpiInventoryProductVersion	OCTET STRING,
	saHpiInventoryModelNumber	OCTET STRING,
	saHpiInventorySerialNumber	OCTET STRING,
	saHpiInventoryPartNumber	OCTET STRING,
	saHpiInventoryFileId		OCTET STRING,
	saHpiInventoryAssetTag		OCTET STRING,
	saHpiInventoryCustomField	OCTET STRING,
	saHpiInventoryOEM		Unsigned32,
	saHpiInventoryRDR		RowPointer
	}

saHpiInventoryEirId OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     
	"Identifier for an entity inventory repository. The  default is
	SAHPI_DEFAULT_INVENTORY_ID  0x00 "
        ::= {saHpiInventoryEntry 1}

saHpiInventoryIndex OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Sub-index for the inventory data resource record.
	This value enumerate n-inventory data resource records for
	a record that has mulitple inventory entires (for the same 
	saHpiInventoryEirId)."
	::= {saHpiInventoryEntry 2}

saHpiInventoryRecordType OBJECT-TYPE
        SYNTAX          INTEGER {
				undefined (0),
				internalUse(177),
                                chassisInfo(178),
                                boardInfo(179),
                                productInfo(180),
                                oem(193)
			}
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     " Inventory Record definitions"
        ::= {saHpiInventoryEntry 3}

saHpiInventoryValidity OBJECT-TYPE
        SYNTAX          INTEGER{
				undefined (0),
				valid(1),
				invalid(2),
				dataOverflow(3)
			}
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Indication as to whether data is complete and valid. "
        ::= {saHpiInventoryEntry 4}

saHpiInventoryAttributes OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE(8|12|260|1000))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	
  	"This attribute represents the current setting of the inventory.  It must
         be interpreted in the context of saHpiInventoryRecordType.

        internalUse  - Variable length opaque data. Max length
		       of data is 1000 octets. 

        chassisInfo  - Chassis informations are represented as follows:
		octet 1-4		- determines the type of chassis:

				other (2), unknown (3), desktop (4),
				lowProfileDesktop (5), pizzaBox (6),
				miniTower (7), tower (8), portable (9),
				laptop (10), notebook (11), handheld (12),
				dockingStation (13), allinone (14),
				subnotebook (15), spaceSaving (16),
				lunchBox (17), mainServer (18),
				expansion (19), subchassis (20),
				busExpansionChassis (21),
				peripheralChassis (22), raidChassis (23),
				rackmount (24)
		octet 4-12	- manufacturing date/time represented
			as 64-bit signed integer from 00:00:00, 
			January 1, 1970, in a 64-bit signed integer. This format
			is sufficient to represent times with nano-second resolution from the
			year 1678 to 2262.
			
			The value -263, which is 0x8000000000000000, is used to indicate
			unknownunspecified time.

		The rest of information is provided in following objects

		saHpiInventoryManufacturer
		saHpiInventoryProductName
		saHpiInventoryProductVersion
		saHpiInventoryModelNumber
		saHpiInventorySerialNumber
		saHpiInventoryPartNumber
		saHpiInventoryFileId
		saHpiInventoryAssetTag
		saHpiInventoryCustomField
			 			
	boardInfo- Textual information regarding the board.
		octet 1-8	- manufacturing date/time represented
			as 64-bit signed integer from 00:00:00, 
			January 1, 1970, in a 64-bit signed integer. This format
			is sufficient to represent times with nano-second resolution from the
			year 1678 to 2262.
			
			The value -263, which is 0x8000000000000000, is used to indicate
			unknownunspecified time.

		The rest of information is provided in following objects

		saHpiInventoryManufacturer
		saHpiInventoryProductName
		saHpiInventoryProductVersion
		saHpiInventoryModelNumber
		saHpiInventorySerialNumber
		saHpiInventoryPartNumber
		saHpiInventoryFileId
		saHpiInventoryAssetTag
		saHpiInventoryCustomField	 
              

		   
	productInfo-The information of the product board represented as follows:

		octet 1-8	- manufacturing date/time represented
			as 64-bit signed integer from 00:00:00, 
			January 1, 1970, in a 64-bit signed integer. This format
			is sufficient to represent times with nano-second resolution from the
			year 1678 to 2262.
			
			The value -263, which is 0x8000000000000000, is used to indicate
			unknownunspecified time.

		The rest of information is provided in following objects

		saHpiInventoryManufacturer
		saHpiInventoryProductName
		saHpiInventoryProductVersion
		saHpiInventoryModelNumber
		saHpiInventorySerialNumber
		saHpiInventoryPartNumber
		saHpiInventoryFileId
		saHpiInventoryAssetTag
		saHpiInventoryCustomField
			                 

	oem	- OEM data represented as follows:
		octet 1-4	- manufacturer ID in network byte order 
		octet 5-n	- variable length data (defined by OEM).
				  max(n) is 260."
        ::= {saHpiInventoryEntry 5}

