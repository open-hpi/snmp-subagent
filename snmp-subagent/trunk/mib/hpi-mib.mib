		saHpiDomainId, 	
		saHpiResourceId
	}
        ::= {saHpiEventLogInfoTable 1}
                                                 
SaHpiEventLogInfoEntry ::= SEQUENCE {

	saHpiEventLogEntries 		Unsigned32,
	saHpiEventLogSize		Unsigned32,
	saHpiEventLogUserEventMaxSize	Unsigned32,
	saHpiEventLogUpdateTimestamp	SaHpiTime,
	saHpiEventLogCurrentTime	SaHpiTime,

	saHpiEventLogIsEnabled		TruthValue,
	
	saHpiEventLogOverflowFlag	TruthValue,
	saHpiEventLogOverflowResetable	TruthValue,
	saHpiEventLogOverflowAction	INTEGER,

	saHpiEventLogOverflowReset	INTEGER,

-- Event Log operations:
	saHpiEventLogDelete		RowStatus,

	saHpiEventLogState		TruthValue

	}

saHpiEventLogEntries OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	read-only
