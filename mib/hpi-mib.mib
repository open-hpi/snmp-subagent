saHpiHotSwapTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SaHpiHotSwapEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table represents management attributes common to all hot swappable 
         entities in the managed platform.  The entries in this table are 
         straight from the HPI spec.  This table builds upon information present 
         in the Entity Table."
    ::= { hpiEvents 4 }


saHpiHotSwapEntry OBJECT-TYPE
    SYNTAX SaHpiHotSwapEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "An entry describing a particular hot swappable entity."
    INDEX {
	saHpiDomainID, saHpiResourceID }
    ::= { saHpiHotSwapTable 1 }


SaHpiHotSwapEntry ::= SEQUENCE {
    saHpiHotSwapIndicator      INTEGER,
    saHpiPowerState            INTEGER,
    saHpiResetState            INTEGER,
    saHpiHotSwapState          INTEGER,
    saHpiPreviousHotSwapState  INTEGER,
    saHpiHotSwapEventSeverity  INTEGER,
    saHpiInsertTimeout         Unsigned32,
    saHpiExtractTimeout        Unsigned32,
    saHpiActionRequest         INTEGER,
    saHpiHotSwapRTP		RowPointer }


saHpiHotSwapIndicator OBJECT-TYPE
    SYNTAX INTEGER {
            off(1),
            on(2) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The indicator (typically an LED) indicating that an entity is ready to
                  be removed."
    REFERENCE 
        "Derived from typedef in the SAI-HPI-A.01.01 page 100."
    ::= { saHpiHotSwapEntry 1 }


saHpiPowerState OBJECT-TYPE
    SYNTAX INTEGER {
            off(1),
            on(2),
            cycle(3) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Indicates the power state of the entity."
    REFERENCE 
        "Derived from typedef in the SAI-HPI-A.01.01 page 100."
    ::= { saHpiHotSwapEntry 2 }


saHpiResetState OBJECT-TYPE
    SYNTAX INTEGER {
            coldReset(1),
            warmReset(2),
            resetAssert(3),
            resetDeassert(4) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "coldReset(write-only): Executes a cold reset on entity.

         warmReset(write-only): Executes a warm reset on entity.

         resetAssert: When written, assert entity's reset line.  When read, 
                      indicates entity's reset line is asserted.

         resetDeassert: When written, deassert entity's reset line.  When read, 
                        indicates entity's reset line is deasserted."
    REFERENCE 
        "Derived from actions defined for function prototypes in the 
         SAI-HPI-A.01.01 pages 73, 74."
    ::= { saHpiHotSwapEntry 3 }


saHpiHotSwapState OBJECT-TYPE
    SYNTAX INTEGER {
            inactive(1),
            insertionPending(2),
            activeHealthy(3),
            activeUnhealthy(4),
            extractionPending(5),
            notPresent(6) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The hot swap state of the resource.

         inactive: Indicates the FRU is no longer active in the system, and that 
                   it has completed the extraction process.

         activeHealthy: Indicates the entity is now an active 
                        member of the domain.  After a FRU completes the 
                        hardware connection process, the associated entity 
                        enters the activeHealthy state if there are no faults 
                        present.  An entity will transition between 
                        activeHealthy and activeUnhealthy as fault conditions                  
                        are asserted or cleared on the FRU.

         activeUnHealthy: Indicates the entity is now an active 
                          member of the domain.  After a FRU completes the 
                          hardware connection process, the associated entity 
                          enters the activeUnhealthy state if there are faults 
                          present.  An entity will transition between 
                          activeHealthy and activeUnhealthy as fault conditions 
                          are asserted or cleared on the FRU.

         extrationPending: Indicates that the entity has requested extraction of 
                           the associated FRU.  Typically an entity enters this 
                           state when an ejector latch is opened or when a hot 
                           swap button is pressed.

         insertionPending: Indicates entity is transitioning from notPresent or 
                           inactive to an active state as a result of being 
                           physically inserted into the system.

         notPresent: The entity is not present in the system."
    REFERENCE 
        "Derived from states described in the SAI-HPI-A.01.01 pages 62, 63."
    ::= { saHpiHotSwapEntry 4 }

saHpiPreviousHotSwapState OBJECT-TYPE
    SYNTAX INTEGER {
            inactive(1),
            insertionPending(2),
            activeHealthy(3),
            activeUnhealthy(4),
            extractionPending(5),
            notPresent(6) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The previous hot swap state of the resource.  See hotSwapState 
         description for explanation of states."
    REFERENCE 
        "Derived from states described in the SAI-HPI-A.01.01 pages 62, 63."
    ::= { saHpiHotSwapEntry 5 }

saHpiHotSwapEventSeverity OBJECT-TYPE
    SYNTAX INTEGER {
            critical(1),
            major(2),
            minor(3),
            informational(4),
            ok(5),
            debug(241) }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Event severity."
    REFERENCE 
        "Derived from typedef saHpiSeverityT described in the SAI-HPI-A.01.01 
         pages 100, 101."
    ::= { saHpiHotSwapEntry 6 }


saHpiInsertTimeout OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Indicates how long to wait (ns) after receiving a FRU insertion 
         indication before the default auto-insertion policy is invoked."
    REFERENCE 
        "Derived from function prototypes in the SAI-HPI-A.01.01 pages 65, 66."
    ::= { saHpiHotSwapEntry 7 }


saHpiExtractTimeout OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Indicates how long to wait (ns) after receiving a FRU extraction 
         indication before the default auto-extraction policy is invoked."
    REFERENCE 
        "Derived from function prototypes in the SAI-HPI-A.01.01 pages 67, 68."
    ::= { saHpiHotSwapEntry 8 }


saHpiActionRequest OBJECT-TYPE
    SYNTAX INTEGER {
            insertion(1),
            extraction(2) }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "This is set when the user wants to invoke either the auto-insertion or
         auto-extraction policy without waiting for a FRU insertion/extraction 
         first."
    REFERENCE 
        "Derived from function prototype in the SAI-HPI-A.01.01 page 69."
    ::= {saHpiHotSwapEntry 9}

saHpiHotSwapRTP	OBJECT-TYPE
	SYNTAX		RowPointer
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"OID for RTP"
	::= {saHpiHotSwapEntry 10}
	
